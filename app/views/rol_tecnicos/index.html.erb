<h1>Listing rol_tecnicos</h1>

<table>
  <thead>
    <tr>
      <th>Tipo rol tec</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rol_tecnicos.each do |rol_tecnico| %>
      <tr>
        <td><%= rol_tecnico.tipo_rol_tec %></td>
        <td><%= link_to 'Show', rol_tecnico %></td>
        <td><%= link_to 'Edit', edit_rol_tecnico_path(rol_tecnico) %></td>
        <td><%= link_to 'Destroy', rol_tecnico, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rol tecnico', new_rol_tecnico_path %>
